{
  "name": "uendel-silveira/api-connection",
  "description": "Pacote de autenticação API para Laravel usando Passport",
  "type": "library",
  "require": {
	"php": "^8.2",
	"laravel/framework": "^9.0|^10.0|^11.0",
	"laravel/passport": "^11.0",
	"phpunit/phpunit": "^9.0",
	"orchestra/testbench": "^8.0"
  },
  "autoload": {
	"psr-4": {
	  "UendelSilveira\\ApiConnection\\AuthApi\\": "src/"
	}
  },
  "autoload-dev": {
	"psr-4": {
	  "UendelSilveira\\ApiConnection\\Tests\\": "tests/"
	}
  },
  "extra": {
	"laravel": {
	  "providers": [
		"uendel-silveira\\api-connection\\AuthApiServiceProvider"
	  ]
	}
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
